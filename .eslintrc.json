{
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "browser": true,
    "es6": false,
    "mocha": true,
    "jasmine": true
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "brace-style": "off",
    "comma-dangle": "off",
    "no-tabs": "off",
    "no-var": "off",
    "new-cap": "off",
    "no-console": "off",
    "require-jsdoc": "off",
    "indent": [
      "error",
      "tab"
    ],
    "no-multiple-empty-lines": [
      2,
      {
        "max": 1,
        "maxBOF": 0,
        "maxEOF": 1
      }
    ]
  },
  "globals": {
    "a11ySuite": true,
    "fixture": true,
    "flush": true,
    "MockInteractions": true,
    "Polymer": true,
    "WCT": true
  }
}
